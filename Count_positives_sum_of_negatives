function countPositivesSumNegatives(input) {
    if (input === null|| input.length < 1) {
    return [];
    }
    
    var positive = 0;
    var negative = 0;
    
    for (var i = 0; i < input.length; i++){
      if (input[i] > 0) {
        positive++;
    }
      else{
        negative += input[i];
    }
    }
    
    var final = [positive, negative];
    return final; 
}
